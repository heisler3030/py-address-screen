# Example configuration file
# Copy this to .env and fill in your actual values

# Required: Your Chainalysis API key
CHAINALYSIS_API_KEY=your_api_key_here

# Optional: API base URL (default: https://api.chainalysis.com)
# CHAINALYSIS_BASE_URL=https://api.chainalysis.com

# Optional: Rate limiting (requests per second, default: 5)
# RATE_LIMIT=5

# Optional: Maximum concurrent requests (default: 10)
# MAX_CONCURRENT_REQUESTS=10

# Optional: Include indirect exposure in screening (default: true)
# INCLUDE_INDIRECT_EXPOSURE=true